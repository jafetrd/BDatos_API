<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="BDatos_API.Properties.Settings.bd_apiConnectionString"
      connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\bd_api.mdf;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.serviceModel>
    <bindings>
      <wsDualHttpBinding>
        <binding name="WSDualHttpBinding_IContenedor" />
      </wsDualHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8000/ServicioTablaContenedor/get"
        binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IContenedor"
        contract="ServiceReference1.IContenedor" name="WSDualHttpBinding_IContenedor">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>